{
  "name": "dash-renderer",
  "version": "1.9.1",
  "description": "render dash components in react",
  "main": "dash_renderer/dash_renderer.min.js",
  "scripts": {
    "prepublishOnly": "rm -rf lib && babel src --extensions=\".ts,.tsx,.js,.jsx\" --out-dir lib --copy-files",
    "private::format.eslint": "eslint --quiet --fix src tests",
    "private::format.prettier": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "private::lint.eslint": "eslint src tests",
    "private::lint.prettier": "prettier \"{src,tests}/**/*.{js,jsx,ts,tsx}\" --list-different",
    "build:js": "webpack",
    "build:dev": "webpack",
    "build:local": "renderer build local",
    "build": "renderer build && npm run prepublishOnly",
    "postbuild": "es-check es5 dash_renderer/*.js",
    "start": "webpack-serve ./webpack.serve.config.js",
    "test": "karma start karma.conf.js --single-run",
    "format": "run-s private::format.*",
    "lint": "run-s private::lint.*"
  },
  "author": "chriddyp",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@plotly/dash-component-plugins": "^1.2.0",
    "cookie": "^0.4.0",
    "cytoscape": "^3.14.1",
    "cytoscape-dagre": "^2.2.2",
    "cytoscape-fcose": "^1.2.3",
    "dependency-graph": "^0.9.0",
    "fast-isnumeric": "^1.1.4",
    "node-polyfill-webpack-plugin": "^1.0.3",
    "prop-types": "15.7.2",
    "radium": "^0.26.0",
    "ramda": "^0.27.0",
    "react": "16.14.0",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "16.14.0",
    "react-json-tree": "^0.11.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@plotly/eslint-config-dash": "0.0.1",
    "@plotly/prettier-config-dash": "0.0.1",
    "@svgr/webpack": "^5.2.0",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.34",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.2.0",
    "es-check": "^5.2.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "karma": "^6.3.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-serve": "^3.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "files": [
    "/dash_renderer/*{.js,.map}",
    "/lib/**"
  ],
  "prettier": "@plotly/prettier-config-dash"
}
