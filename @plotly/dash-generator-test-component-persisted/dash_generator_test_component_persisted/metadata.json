{
  "src/components/MyPersistedComponent.js": {
    "description": "MyComponent description",
    "displayName": "MyPersistedComponent",
    "methods": [],
    "props": {
      "id": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The id of the component"
      },
      "style": {
        "type": {
          "name": "custom",
          "raw": "PropTypes.shape"
        },
        "required": false,
        "description": "The style"
      },
      "value": {
        "type": {
          "name": "string"
        },
        "required": false,
        "description": "The value to display",
        "defaultValue": {
          "value": "''",
          "computed": false
        }
      },
      "persistence": {
        "type": {
          "name": "union",
          "value": [
            {
              "name": "bool"
            },
            {
              "name": "string"
            },
            {
              "name": "number"
            }
          ]
        },
        "required": false,
        "description": ""
      },
      "persisted_props": {
        "type": {
          "name": "arrayOf",
          "value": {
            "name": "enum",
            "value": [
              {
                "value": "'value'",
                "computed": false
              }
            ]
          }
        },
        "required": false,
        "description": "Properties whose user interactions will persist after refreshing the\ncomponent or the page.",
        "defaultValue": {
          "value": "['value']",
          "computed": false
        }
