{
  "name": "dash-renderer",
  "version": "1.10.0",
  "description": "render dash components in react",
  "main": "build/dash_renderer.min.js",
  "scripts": {
    "prepublishOnly": "rm -rf lib && babel src --extensions=\".ts,.tsx,.js,.jsx\" --out-dir lib --copy-files",
    "private::format.eslint": "eslint --quiet --fix src tests",
    "private::format.prettier": "prettier --write \"{src,tests}/**/*.{js,jsx,ts,tsx}\"",
    "private::lint.eslint": "eslint src tests",
    "private::lint.prettier": "prettier \"{src,tests}/**/*.{js,jsx,ts,tsx}\" --list-different",
    "build:js": "webpack",
    "build:dev": "webpack",
    "build:local": "renderer build local",
    "build": "renderer build && npm run prepublishOnly",
    "postbuild": "es-check es5 ../deps/*.js",
    "test": "karma start karma.conf.js --single-run",
    "format": "run-s private::format.*",
    "lint": "run-s private::lint.*"
  },
  "author": "chriddyp",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@plotly/dash-component-plugins": "^1.2.0",
    "cookie": "^0.4.1",
    "cytoscape": "^3.19.0",
    "cytoscape-dagre": "^2.3.2",
    "cytoscape-fcose": "^2.1.0",
    "dependency-graph": "^0.11.0",
    "fast-isnumeric": "^1.1.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prop-types": "15.7.2",
    "radium": "^0.26.1",
    "ramda": "^0.27.1",
    "react": "16.14.0",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "16.14.0",
    "react-json-tree": "^0.15.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@plotly/eslint-config-dash": "0.0.1",
    "@plotly/prettier-config-dash": "0.0.1",
    "@svgr/webpack": "^5.5.0",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@types/ramda": "0.27.6",
    "@types/react": "^16.14.8",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.31",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "css-loader": "^5.2.6",
    "es-check": "^5.2.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "whatwg-fetch": "^3.6.2"
  },
  "files": [
    "build/*{.js,.map}",
    "/lib/**"
  ],
  "prettier": "@plotly/prettier-config-dash"
}
